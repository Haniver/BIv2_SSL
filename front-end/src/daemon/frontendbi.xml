<service>
	<id>frontendbi.exe</id>
	<name>Front End BI</name>
	<description>Front End para el BI de Omnicanal</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>C:\BI_Omnicanal\front-end\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\BI_Omnicanal\front-end\src\App.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Front End BI wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>false</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>CHAZBDDEV</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\BI_Omnicanal\front-end</workingdirectory>
</service>